<svg width="325" height="{{ include.height }}" viewBox="0 0 325 {{ include.height }}">
  <defs>
    <mask id="mask-{{ include.maskid }}">
      <rect fill="#777" width="100%" height="100%"></rect>
      <rect class="loading maskr2" fill="hsla(200,0%,10%,.6)" width="100%" height="100%"></rect>
    </mask>
    <clippath id="clip-{{ include.maskid }}">
      <rect id="clip-{{ include.maskid }}-pos" x="0" y="0" width="0" height="0"></rect>
      <rect id="clip-{{ include.maskid }}-neg" x="0" y="0" width="0" height="0"></rect>
    </clippath>
  </defs>
  <rect class="loading" mask="url(#mask-{{ include.maskid }})" fill="#dadada" width="100%" height="100%"></rect>
</svg>

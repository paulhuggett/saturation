<div>Input {{ input }}</div>
<div class="keep-together">
  <label for="frequency{{ input }}" class="small-text">Frequency:</label>
  <input type="range" class="small-text" list="ticksfrequency{{ input }}"
         min="0.1" max="4.0" value="2.0" step="0.05"
         id="frequency{{ input }}">
  <datalist id="ticksfrequency{{ input }}">
    <option value="0.1"></option>
    <option value="1"></option>
    <option value="2"></option>
    <option value="3"></option>
    <option value="4"></option>
  </datalist>
  <div id="freqvalue{{ input }}" class="num small-text"></div>
</div>
<div class="keep-together">
  <label for="amplitude{{ input }}" class="small-text">Amplitude:</label>
  <input type="range" class="small-text" list="ticksamplitude{{ input }}"
         min="0.0" max="1.0" value="0.9" step="0.01"
         id="amplitude{{ input }}">
  <datalist id="ticksamplitude{{ input }}">
    <option value="0"></option>
    <option value="0.2"></option>
    <option value="0.4"></option>
    <option value="0.6"></option>
    <option value="0.8"></option>
    <option value="1.0"></option>
  </datalist>
  <div id="ampvalue{{ input }}" class="num small-text"></div>
</div>
